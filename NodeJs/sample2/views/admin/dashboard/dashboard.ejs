        type="text/css" />
    <link href="<%= process.env.AAP_URL %>public/plugins/datatables/buttons.bootstrap4.min.css" rel="stylesheet"
        type="text/css" />
    <!-- Responsive datatable examples -->
    <link href="<%= process.env.AAP_URL %>public/plugins/datatables/responsive.bootstrap4.min.css" rel="stylesheet"
        type="text/css" />
    <!--sweetalert-->
    <link href="<%= process.env.AAP_URL %>public/plugins/sweet-alert2/sweetalert2.min.css" rel="stylesheet"
        type="text/css">
    <%- contentFor('body') %>
        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <h4 class="page-title">Dashboard</h4>
                            <!--                                     <ol class="breadcrumb">
                                        <li class="breadcrumb-item active">
                                            Welcome to Dashboard
                                        </li>
                                    </ol> -->

                        </div>
                    </div>
                </div>

                <div id="alert_msg" style="display: none;">
                </div>

                <div class="row">
                    <div class="col-12">
                        <div class="card m-b-20">
                            <div class="card-body">
                                <table id="datatable-buttons"
                                    class="table table-striped table-hover nowrap"
                                    style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                    <thead>
                                        <tr>
                                            <th>No</th>
                                            <th>User Name</th>
                                            <th>Therapist Name</th>
                                            <th>Session Date</th>
                                            <th>Session Start Time</th>
                                            <th>Session End Time</th>
                                            <th>Status</th>
                                            <th>Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% data.forEach(rec=> { %>
                                            <% if(rec.status==1){ %>
                                                <% var date=new Date(); date.setDate(date.getDate()); %>
                                                    <% if(rec.session_date >=
                                                        date.toISOString().substr(0, 10)) { %>
                                                        <tr>
                                                            <td>
                                                                <%= rec.id %>
                                                            </td>
                                                            <td>
                                                                <%= rec.user_fname+" "+rec.user_lname %></td>
                                                    <td><%= rec.therapist_fname+" "+rec.therapist_lname %></td>
                                                    
                                                    <td><%= rec.session_date %></td>
                                                    <td><%= rec.session_start_time %></td>
                                                    <td><%= rec.session_end_time %></td>
                                                    <td>
                                                        <%
                                                            var status_class = "";
                                                            var status = "";
                                                            if(rec.status == 0)
                                                            {
                                                                status_class = " warning"; var status="Pending" ; }
                                                                    else if(rec.status==1) { status_class="success" ;
                                                                    status="Booked" ; }else if(rec.status==2) {
                                                                    status_class="danger" ; status="Cancelled" ; }else
                                                                    if(rec.status==3) { status_class="danger" ;
                                                                    status="Reject" ; } %>
                                                                    <span class="badge badge-<%=status_class%>"
                                                                        style="font-size: 120%;">
                                                                        <%=status%>
                                                                    </span>
                                                            </td>
                                                            <td>
                                                                <div class="btn-group m-b-10">
                                                                    <button type="button"
                                                                        class="btn btn-primary dropdown-toggle"
                                                                        data-toggle="dropdown" aria-haspopup="true"
                                                                        aria-expanded="false">Action</button>
                                                                    <% if(rec.status !=2){ %>
                                                                        <div class="dropdown-menu">
                                                                            <a class="dropdown-item"
                                                                                href="JavaScript:void(0)"
                                                                                onclick="share_link('<%= rec.id %>')">Share Meeting Link</a>
                                                                            <div class="dropdown-divider"></div>
                                                                            <a class="dropdown-item"
                                                                                href="JavaScript:void(0)"
                                                                                onclick="join_link('<%= rec.id %>')">Join Meeting</a>
                                                                            <div class="dropdown-divider"></div>
                                                                            <a class="dropdown-item"
                                                                                href="JavaScript:void(0)"
                                                                                onclick="cancel_appointment('<%= rec.id %>')"
                                                                                title="Cancel">Cancel Meeting </a>
                                                                        </div>
                                                                        <% } %>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <% } } %>
                                                            <% }); %>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div> <!-- end col -->
                </div>
                <!--  Modal content for the above example -->
                <div class="modal fade" id="share_link_modal" tabindex="-1" role="dialog"
                    aria-labelledby="myLargeModalLabel" aria-hidden="true">
                    <div class="modal-dialog">
                        <form class="" method="post" id="share_link"
                            action="<%= process.env.AAP_URL %>admin/appointment/sharelink">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title mt-0" id="myLargeModalLabel">Share link
                                    </h5>
                                    <button type="button" class="close" data-dismiss="modal"
                                        aria-hidden="true">×</button>
                                </div>
                                <div class="modal-body">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="card m-b-20">
                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <div class="form-group">
                                                                <label>Email</label>
                                                                <input type="email" name="email"
                                                                    class="form-control input" id="f_name" required
                                                                    parsley-type="email" placeholder="Mail ID">
                                                                <input type="hidden" name="appointment_id" class="input"
                                                                    id="appointment_id">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary waves-effect"
                                        data-dismiss="modal">Cancel</button>
                                    <button type="submit"
                                        class="btn btn-primary waves-effect waves-light">Submit</button>
                                </div>
                            </div><!-- /.modal-content -->
                        </form>
                    </div><!-- /.modal-dialog -->
                </div><!-- /.modal -->
