                            <% if(auth_data.role=="super_admin" ){ %>
                                <div class="state-information d-none d-sm-block">
                                    <div class="state-graph">
                                        <button type="button" onclick="reset_form()" class="btn btn-success waves-effect" data-toggle="modal"
                                            data-target=".therapist_modal"> <i class=" fas fa-plus "></i> Create
                                            Therapist</button>
                                    </div>
                                </div>
                                <% } %>
                        </div>
                    </div>
                </div>
                <!-- end row -->
                <div id="alert_msg" style="display: none;">
                </div>

                <div class="row">
                    <div class="col-12">
                        <div class="card m-b-20">
                            <div class="card-body">
                                <table id="datatable-buttons"
                                    class="table table-striped table-hover nowrap"
                                    style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                                    <thead>
                                        <tr>
                                            <th>No</th>
                                            <th>F Name</th>
                                            <th>L Name</th>
                                            <th>Mail ID</th>
                                            <%- (auth_data.role=="super_admin" )? '<th>Contact No</th>' : '' %>
                                                <th>Free sessions</th>
                                                <th>Paid Sessions</th>
                                                <th>Total Sessions</th>
                                                <th>Status</th>
                                                <%- (auth_data.role=="super_admin" )? '<th>Action</th>' : '' %>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% therapistlist.forEach(data=> { %>
                                            <tr>
                                                <td>
                                                    <%= data.id %>
                                                </td>
                                                <td>
                                                    <%= data.f_name %>
                                                </td>
                                                <td>
                                                    <%= data.l_name %>
                                                </td>
                                                <td>
                                                    <%= data.email %>
                                                </td>
                                                <%- (auth_data.role=="super_admin" )? '<td>' +data.mobile+'</td>' : ''
                                                    %>
                                                    <td>-</td>
                                                    <td><%= data.book_schedule %></td>
                                                    <td><%= data.book_schedule %></td>
                                                    <td>
                                                        <% if(data.status==1){ %>
                                                            <span class="badge badge-success"
                                                                style="font-size: 120%;">Active</span>
                                                            <% }else if(data.status==2){ %>
                                                                <span class="badge badge-warning"
                                                                    style="font-size: 120%;">Pending</span>
                                                                <% }else{ %>
                                                                    <span class="badge badge-danger"
                                                                        style="font-size: 120%;">Inactive</span>
                                                                    <% } %>
                                                    </td>
                                                    <% if(auth_data.role=="super_admin" ){ %>
                                                        <td>
                                                            <% if(data.status != 2){ %>
                                                            <button type="submit"
                                                                class="btn btn-outline-warning waves-effect waves-light"
                                                                data-toggle="tooltip" onclick="edit_data('<%= data.id %>')"
                                                                data-placement="top" title="Edit"><i
                                                                    class="far fa-edit"></i></button>
                                                                    <% } %>
                                                            <button type="submit"
                                                                class="btn btn-outline-danger waves-effect waves-light "
                                                                data-toggle="tooltip" onclick="delete_data('<%= data.id %>')"
                                                                data-placement="top" title="Delete"><i
                                                                        class="far fa-trash-alt"></i></button>
                                                            <% if(data.status==2){ %>
                                                                
                                                              <button type="submit"
                                                            class="btn btn-outline-info waves-effect waves-light "
                                                            data-toggle="tooltip" onclick="resend_email('<%= data.id %>')"
                                                            title="Resend Email "><i class="fa fa-envelope" aria-hidden="true"></i>
                                                        </button> 
                                                        <% } %>
                                                        </td>
                                                        <% } %>
                                            </tr>
                                            <% }); %>
                                </table>
                            </div>
                        </div>
